{
    "name": "AdvancedImageSearch",
    "description": "Convert image to text",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
      "persistent": true,
      "scripts": ["js/tesseract.min.js","js/background.js"]
    },
    "permissions":[
      "contextMenus",
      "activeTab"
    ],
    "browser_action": {
      "default_popup": "index.html"
    },
    "content_security_policy": "script-src 'self' https://unpkg.com; object-src 'self'"
  }